<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

	<extension point="org.eclipse.equinox.http.registry.servlets">
		<servlet
            alias="//swnhelper"
            class="frostillicus.swn.app.AppServletFiltered">
		</servlet>
	</extension>


	<!-- Auto start of the OSGi plugin - This is optional, but let the services start when the server is started  -->
	<extension point="com.ibm.commons.Extension">
		<service type="com.ibm.xsp.adapter.serviceFactory" class="frostillicus.swn.app.xsp.AutoStartServiceFactory" />
	</extension>

	<!-- Application specific Plugin -->
	<extension point="darwino.eclipse.darwinoPlugin">
		<service class="frostillicus.swn.app.AppPlugin" />
	</extension>

	<!-- Web resources Plugins -->
	<extension point="darwino.eclipse.darwinoPlugin">
		<service class="com.darwino.platform.web.PluginDarwinoWeb" />
	</extension>

</plugin>
